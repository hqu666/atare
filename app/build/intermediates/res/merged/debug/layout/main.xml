<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:tools="http://schemas.android.com/tools"
              xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:ads="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              tools:context=".AtarekunnActivity"
              android:weightSum="1">
    <!--xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top" >

        <Spinner
            android:id="@+id/kujisyuSP"
            android:layout_width="212dp"
            android:layout_height="46dp"
            android:layout_weight="1.11"
            android:entries="@array/kujiSyuArray"
            android:spinnerMode="dropdown"
            android:textAppearance="?android:attr/textAppearanceLarge"
            tools:listitem="@android:layout/simple_spinner_dropdown_item" />

        <Spinner
            android:id="@+id/repetSp"
            android:layout_width="130dp"
            android:layout_height="46dp"
            android:minHeight="46dp"
            android:spinnerMode="dropdown"
            android:textAppearance="?android:attr/textAppearanceLarge"
            tools:listitem="@android:layout/simple_spinner_dropdown_item" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/kahen_hyouji"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center"
            android:layout_weight="1"
            android:gravity="right"
            android:text="@string/main_tf_hyoujisuu"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <Spinner
            android:id="@+id/maxValSP"
            android:layout_width="127dp"
            android:layout_height="44dp"
            android:layout_weight="0.03"
            android:spinnerMode="dropdown"
            android:textAppearance="?android:attr/textAppearanceLarge"
            tools:listitem="@android:layout/simple_spinner_dropdown_item" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/green_android2" >

        <LinearLayout
            android:id="@+id/ippatu_Layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@color/black" >

            <TextView
                android:id="@+id/f1mainTF"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@color/red2"
                android:clickable="true"
                android:gravity="center"
                android:hint="@string/_1_2_3_4_5_43"
                android:nextFocusUp="@id/kujisyuSP"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="32dp" />

            <TextView
                android:id="@+id/f1SubTF"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@color/red2_r"
                android:hint="8,9"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/black"
                android:textSize="32dp" />

        </LinearLayout>
    </FrameLayout>

    <LinearLayout
        android:id="@+id/ruisekiLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/saidaiTF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/comon_max"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/ruisekiSuuTF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="100"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.66"
            android:text="@string/main_tf_kaimaderuiseki"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/aCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:text="43"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:text="/"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/tCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:text="43"
            android:textAppearance="?android:attr/textAppearanceMedium" />
    </LinearLayout>

    <ScrollView
        android:id="@+id/lank_sv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fastScrollEnabled="true" >

        <TextView
            android:id="@+id/lank1data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="1"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/red" >

        </TextView>
    </ScrollView>

    <WebView
        android:layout_width="match_parent"
        android:layout_height="358dp"
        android:id="@+id/webView"
        android:layout_weight="1"
        />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="bottom|left"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id"
        >
    </com.google.android.gms.ads.AdView>
    <!--ads:testDevices　と　ads:loadAdOnCreatは廃止-->
    <!--ads:adUnitId="@string/banner_ad_unit_id"-->


    <net.nend.android.NendAdView
        android:id="@+id/nend"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="1dp"
        NendAdjustSize="true"
        NendApiKey="@string/nend_apiKey"
        NendSpotId="@string/nend_spotID"
        />

